<?xml version="1.0" encoding="utf-8"?>
<ripple android:color="@color/colorRipple" xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/mask">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorRipple" />
            <corners android:radius="@dimen/corner_radius_main" />
        </shape>
    </item>

    <item>
        <shape>
            <solid android:color="@color/white" />
            <corners android:radius="@dimen/corner_radius_main" />
        </shape>
    </item>
</ripple>
